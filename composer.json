{
  "name": "xterr/php-caencodes",
  "description": "Caen Codes",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Razvan Ceana",
      "email": "razvan@ceana.ro"
    }
  ],
  "require": {
    "php": ">=7.1 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "captainhook/captainhook": "^5.24",
    "ergebnis/composer-normalize": "^2.45",
    "phpunit/phpunit": "^10.0",
    "ramsey/conventional-commits": "^1.5",
    "symfony/console": "^6.0 || ^7.0",
    "symfony/translation": "^6.0 || ^7.0",
    "symfony/yaml": "^6.0 || ^7.0",
    "vimeo/psalm": "^5.26"
  },
  "autoload": {
    "psr-4": {
      "Xterr\\CaenCodes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xterr\\CaenCodes\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "composer-normalize": {
      "indent-size": 2,
      "indent-style": "space"
    }
  }
}
